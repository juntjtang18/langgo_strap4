{
  "kind": "collectionType",
  "collectionName": "topics",
  "info": {
    "singularName": "topic",
    "pluralName": "topics",
    "displayName": "topic",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "difficulty_level": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::difficulty-level.difficulty-level",
      "inversedBy": "topics"
    },
    "description": {
      "type": "string"
    },
    "questions": {
      "type": "component",
      "repeatable": true,
      "component": "a.question"
    },
    "tags": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean",
      "default": true
    },
    "proficiency_level": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::proficiency-level.proficiency-level",
      "inversedBy": "topics"
    },
    "target_vocab": {
      "type": "json"
    },
    "target_patterns": {
      "type": "json"
    },
    "grammar_point": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::grammar-point.grammar-point",
      "inversedBy": "topics"
    },
    "mode_hint": {
      "type": "enumeration",
      "enum": [
        "auto",
        "practice",
        "scenario"
      ],
      "default": "auto"
    },
    "role_name": {
      "type": "string"
    },
    "role_context": {
      "type": "string"
    }
  }
}
